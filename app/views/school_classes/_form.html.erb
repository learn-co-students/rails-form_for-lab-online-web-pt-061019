<%= form_for @school_class do %>
  <label for="school_class_title">Title:</label><br>
  <%= text_field_tag :'school_class[title]', @school_class.id != nil ? @school_class.title : '' %><br><br>
  <label for="school_class_room_number">Room Number:</label><br>
  <%= text_field_tag :'school_class[room_number]', @school_class.id != nil ? @school_class.room_number : '' %><br><br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag @school_class.id != nil ? 'Update School class' : 'Create School class' %>
<% end %>